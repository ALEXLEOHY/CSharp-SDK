<Window x:Class="NLECloudSDKSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="云平台全API DEMO演示" Height="557" Width="866">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="261" Margin="0,10,0,0" VerticalAlignment="Top" Width="850">
            <TabItem Header="帐号API" Height="21" VerticalAlignment="Bottom">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,-5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Content="说明：&#xA;1、App.config中ServerDomain定义云平台接口的API域名，默认为新大陆物联网云平台API域名，测试环境或私有云请更改为自己的&#xD;&#xA;2、用户名与密码分别是云平台注册的帐号与密码，需一致，否则登录会失败。登录后会返回一个Token，该Token为所有其它API接口&#xA;调用的凭证，所以调用其它API必须要先登录然后得到这个Token，再调用其它API时带上这个Token才能正确调用其它API&#xA;3、接下来每个API的调用都会在代码中加以注释说明，请参考各个方法&#xA;4、每个API会在下方分别列出请求的原始JSON格式或返回的JSON格式供参考，至于每个JSON转成对象，则看个个调用的代码" HorizontalAlignment="Left" Margin="28,81,0,0" VerticalAlignment="Top" Width="755" Height="106"/>
                    <Button x:Name="btnLogin" Content="API：用户登录" HorizontalAlignment="Left" Margin="306,53,0,0" VerticalAlignment="Top" Width="107" Click="btnLogin_Click"/>
                    <TextBox x:Name="txtUserName" HorizontalAlignment="Left" Height="23" Margin="78,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="89" VerticalContentAlignment="Center" Text="18965562233"/>
                    <Label Content="用户名" HorizontalAlignment="Left" Margin="27,50,0,0" VerticalAlignment="Top"/>
                    <Label Content="密码" HorizontalAlignment="Left" Margin="179,50,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtToken" HorizontalAlignment="Left" Height="23" Margin="107,189,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="729" VerticalContentAlignment="Center"/>
                    <Label Content="登录Token" HorizontalAlignment="Left" Margin="27,188,0,0" VerticalAlignment="Top" Width="75"/>
                    <Label Content="API接口" HorizontalAlignment="Left" Margin="29,20,0,0" VerticalAlignment="Top" Width="58"/>
                    <TextBox x:Name="txtLoginAPI" HorizontalAlignment="Left" Height="23" Margin="87,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="278" VerticalContentAlignment="Center" IsReadOnly="True" Text="v2/Account/Login"/>
                    <PasswordBox x:Name="txtPasswd" HorizontalAlignment="Left" Margin="218,53,0,0" VerticalAlignment="Top" Width="69" Height="23" Password="123456"/>
                </Grid>
            </TabItem>
